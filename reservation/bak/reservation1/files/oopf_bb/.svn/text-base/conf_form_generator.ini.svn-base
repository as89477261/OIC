[Main]
DefaultReader = ini
DefaultInputType = text
IncludedFiles =

[ProfileReader]
ini = CfprIni                   ;extension = handling class
xml = CfprXml

[OutputType]
basic = BasicFormInputType
html = HtmlFormInputType

;add info field too! (it's not input but useful)
[HtmlFormInputType]
default = CFormInputHtml
text = CfihText
password = CfihPassword
hidden = CfihHidden
file = CfihFileUpload
radio = CfihRadio
select = CfihSelect
checkbox = CfihCheckBox
textarea = CfihTextArea

[BasicFormInputType]
default = CFormInput
text = CfiText
password = CfiPassword
hidden = CfiHidden
file = CfiFileUpload
radio = CfiRadio
select = CfiSelect
checkbox = CfiCheckBox
textarea = CfiTextArea

[FormInputChoice]
fixed = CficmFixed
output = CficmOutputArray
; table and const will be installed by oopf because they are depending on how framework retrieve those data.